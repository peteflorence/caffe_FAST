net: 'fcn-16s-pascal-TRAIN.prototxt'
#test_interval: 20
#test_iter:     2
#weight_decay:  0.0005
lr_policy: "fixed"
# lr for unnormalized softmax -- see train_val definition
base_lr: 1e-10 
momentum:      0.99
gamma:         0.1
max_iter:      500000
display:       10
snapshot:      10000
snapshot_prefix: 'fcn16/FCN_16S_snapshot'
solver_mode:   GPU
debug_info:    0

