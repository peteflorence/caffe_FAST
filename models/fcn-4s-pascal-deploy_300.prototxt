layer { 
  bottom: "conv4_3" 
  top: "conv4_3" 
  name: "relu4_3" 
  type: "ReLU"
}
layer { 
  bottom: "conv4_3" 
  top: "pool4" 
  name: "pool4" 
  type: "Pooling"
  pooling_param { 
