name: 'FCN'
input: 'data'
input_dim: 1
input_dim: 3
input_dim: 300
input_dim: 300

layer { 
  bottom: "data"
  top: "conv1_1"
  name: "conv1_1" 
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param { 
    engine: CAFFE 
    num_output: 64 
    pad: 10 
    kernel_size: 3 
